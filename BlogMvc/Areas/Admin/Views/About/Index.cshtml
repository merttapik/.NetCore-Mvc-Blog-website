@using EntityLayer.Concrete
@model List<About>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>About</h1>
<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>About Details</th>
      
        <th>Edit</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.AboutId</td>
            <td>@item.AboutDetails</td>
           
            <td><a href="/Admin/About/Edit/@item.AboutId" class="btn btn-warning">Edit </a> </td>
        </tr>
    }
</table>

<br />

